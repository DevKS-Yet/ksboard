<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ksboard.user.UserMapper">

    <insert id="login" parameterType="com.ksboard.user.UserVo">
        INSERT INTO user
        values (#{id}, #{pw}, sysdate());
    </insert>

